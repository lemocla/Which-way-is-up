{
    "version": 2,
    "builds": [
      {
        "src": "whichwayisup/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
 
      {
        "src": "/(.*)",
        "dest": "whichwayisup/wsgi.py"
      }
    ],
    "functions": {
      "*/wsgi.py.py": {
        "excludeFiles": "public/**"
      }
    }
  }
  